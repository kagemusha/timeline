<div class="GameForms">
  <div class="CreateGameForm GameForm">
    {{input class="PlayerName" placeholder="Player name" value=createGamePlayer}}
    {{input class="NewGameCode" placeholder="Game code" value=newGameCode}}
    <button class="CreateGameButton" {{action 'createGame'}} >Create Game</button>
  </div>

  <div class="JoinGameForm GameForm">
    {{input class="PlayerName" placeholder="Player name" value=joinGamePlayer}}
    {{input class="NewGameCode" placeholder="Game code" value=joinGameCode}}
    <button class="JoinGameButton" {{action 'joinGame'}} >Join Game</button>
  </div>
</div>

